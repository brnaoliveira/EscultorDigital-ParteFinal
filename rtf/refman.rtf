358             {\cf20 // Craindo o escultor 3D com Voxels zerados}\par
359             {\cf19 for} ({\cf18 int} k=0;k<NumP;k++) \{\par
360                 vector<vector<Voxel>> aux1;\par
361                 {\cf19 for}({\cf18 int} i=0;i<NumL; i++)\{\par
362                     vector<Voxel> aux2;\par
363                     {\cf19 for}({\cf18 int} j=0;j<NumC; j++)\{\par
364                         aux2.push_back(voxel_zero);\par
365                     \}\par
366                     aux1.push_back(aux2);\par
367                 \}\par
