369             \}\par
370 \par
371             {\cf20 // Definido como a primeira tela de desenho o plano zero(XY)}\par
372             plano_atual = painter_sculptor[0];\par
373 \par
374             {\cf20 // Removendo o escultor anterior anterior}\par
